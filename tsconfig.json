{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "loading-skeleton": [
        "dist/loading-skeleton/loading-skeleton",
        "dist/loading-skeleton"
      ],
      "ngx-suspense": [
        "dist/ngx-suspense/ngx-suspense",
        "dist/ngx-suspense"
      ],
      "ngx-error-boundary": [
        "dist/ngx-error-boundary/ngx-error-boundary",
        "dist/ngx-error-boundary"
      ]
    },
    "resolveJsonModule": true
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}